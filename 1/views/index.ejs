<body>
  <%= title %>
 


  <% if (second % 2===0) { %>
    <ul>
      <!-- List goes here if current second is even. -->
      <%items.forEach((fruit)=> { %>
        <li>
          <%=fruit %>
        </li>
        <%}) %>

          <!-- Otherwise it should display the following: -->
          <!-- <p>No items to display</p> -->
    </ul>
    <% } else {%>
      <p>No items to display</p>
      <% } %>









  <%- htmlContent %>
  <%- include('footer.ejs') %>
  
</body>
</html>